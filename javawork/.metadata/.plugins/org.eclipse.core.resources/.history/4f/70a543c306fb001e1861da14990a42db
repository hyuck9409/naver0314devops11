package programmers;

import java.util.Arrays;

class Solution {
    public int[] solution(int[] numbers) {
        int[] answer = new int[numbers.length]; // numbers 배열과 같은 길이의 새로운 배열 생성
        
        for (int i = 0; i < answer.length; i++) {
            answer[i] = numbers[i] * 2; // 각 원소를 두 배하여 새로운 배열에 저장
        }
        
        return answer; // 새로운 배열 반환
    }
}