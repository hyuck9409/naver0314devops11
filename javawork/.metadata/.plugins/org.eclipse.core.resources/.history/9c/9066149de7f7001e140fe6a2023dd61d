package day0408;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
    	int A,B;
       A=10;
       B=15;
       if(A>B) {
    	   System.out.println(">");
       }
       else if(A<B) {
    	   System.out.println("<");
       }
       else if(A==B) {
    	   System.out.println("==");
       }
       
    }
}