package day0408;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a,b,c,d,e,f;
		int A,B;
		a=sc.nextInt();
		b=sc.nextInt();
		c=sc.nextInt();
		
		d=sc.nextInt();
		e=sc.nextInt();
		f=sc.nextInt();
		A=(100*a+10*b+c);
		System.out.println(A+".....(1)");
		B=(100*d+10*e+f);
		System.out.println(B+".....(2)");
		
		System.out.println(f*c+f*(10*b)+f*(100*a)+".....(3)");
		System.out.println(e*c+e*(10*b)+e*(100*a)+".....(4)");
		System.out.println(d*c+d*(10*b)+d*(100*a)+".....(5)");
		
	}

}
