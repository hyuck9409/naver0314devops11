package day0408;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a,b,c,d,e,f;
		int A,B;
		a=sc.nextInt();
		b=sc.nextInt();
		c=sc.nextInt();
		
		d=sc.nextInt();
		e=sc.nextInt();
		f=sc.nextInt();
		A=(100*a+10*b+c);
		System.out.println(A+".....(1)");
		B=(100*d+10*e+f);
		System.out.println(B+".....(2)");
		
		int result=((f*c)+(f*(10*b))+(f*(100*a))+
				(10*e*c)+(10*e*(10*b))+(10*e*(100*a))+
				(100*d*c)+(100*d*(10*b))+(100*d*(100*a)));
		System.out.println((f*c)+(f*(10*b))+(f*(100*a))+".....(3)");
		System.out.println((10*e*c)+(10*e*(10*b))+(10*e*(100*a))+".....(4)");
		System.out.println((100*d*c)+(100*d*(10*b))+(100*d*(100*a))+".....(5)");
		
		System.out.println(result);
		
		 
	}

}
